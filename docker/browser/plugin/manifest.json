{
  "name": "Browshell Executor",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
